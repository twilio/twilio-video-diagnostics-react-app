      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "26.0.23",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "husky": "4.3.8",
    "jest-junit": "12.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.4",
    "typescript": "4.3.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
