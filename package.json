{
  "name": "twilio-video-diagnostics-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@twilio-labs/serverless-api": "^5.0.0",
    "@twilio/rtc-diagnostics": "^1.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.1",
    "dotenv": "^10.0.0",
    "http-proxy-middleware": "^2.0.1",
    "immer": "9.0.3",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "twilio": "^3.63.1",
    "twilio-video": "https://github.com/twilio/twilio-video.js/archive/refs/tags/2.17.0-rc3.tar.gz",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test:serverless": "jest -c jest.serverless.config.js",
    "test:ci": "npm run test:ci:app && npm run test:ci:serverless",
    "test:ci:app": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "test:ci:serverless": "jest -c jest.serverless.config.js --ci --runInBand",
    "serverless:deploy": "npm run build && node serverless/scripts/deploy.js",
    "serverless:remove": "node serverless/scripts/remove.js",
    "serverless:list": "node serverless/scripts/list.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-shadow": "off",
      "@typescript-eslint/no-shadow": [
        "warn"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "@testing-library/react-hooks": "^7.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "husky": "^4.3.8",
    "jest-junit": "^12.1.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "stdout-stderr": "^0.1.13",
    "superagent": "^6.1.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.3.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
